{
    "scopeName": "source.gentemp",
    "patterns": [{ "include": "#expression" }],
    "repository": {
      "expression": {
        "patterns": [
            { "include": "#boxname" },
            { "include": "#line-devider" },
            { "include": "#object-param" },
            { "include": "#object-param-substitution" },
            { "include": "#object-param-function" }
        ]
      },
      "boxname": {
        "match": "ХРАНИЛИЩЕ_ОБЪЕКТОВ|ШАБЛОННЫЙ_ВИД|РЕШЕНИЕ|СЛУЖЕБНОЕ|ТЕСТОВЫЕ_ДАННЫЕ",
        "name": "markup.bold"
      },
      "line-devider": {
        "match": "-----*\n",
        "name": "markup.bold"
      },
      "object-param": {
        "match": "[а-я|А-Я|a-z|A-Z|0-9|_]+[ ]*:",
        "name": "entity.name.type"
      },
      "object-param-substitution": {
        "match": "@[а-я|А-Я|a-z|A-Z|0-9|_|@]+@",
        "name": "markup.italic"
      },
      "object-param-function": {
        "match": "(#rnd|#genAE|#getAEcode|#lua)\\(.*\\)",
        "name": "entity.name.function"
      }
    }
  }